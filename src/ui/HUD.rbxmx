			<Properties>
				<ProtectedString name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Net = require(ReplicatedStorage.Shared.Net)

local GetGameState = Net.GetRemoteFunction("GetGameState")

local gui = script.Parent
local moneyLabel = gui.MoneyLabel
local queueLabel = gui.QueueLabel
local orderLabel = gui.OrderLabel
local serveButton = gui.ServeButton

local serving = false

print("[UI] HUD started")

local function updateHUD()
	local state = GetGameState:InvokeServer()
	if not state then return end

	-- Ð¾Ð±Ð½Ð¾Ð²Ð»ÑÐµÐ¼ Ñ‚ÐµÐºÑÑ‚
	moneyLabel.Text = "Money: " .. tostring(state.Money or 0)
	queueLabel.Text = "Queue: " .. tostring(state.QueueSize or 0)
	orderLabel.Text = "Order: " .. tostring(state.CurrentOrder or "â€”")

	-- ðŸ”’ ÑƒÐ¿Ñ€Ð°Ð²Ð»ÐµÐ½Ð¸Ðµ ÐºÐ½Ð¾Ð¿ÐºÐ¾Ð¹ Serve
	if state.HasWaiter then
		serveButton.Active = false
		serveButton.AutoButtonColor = false
		serveButton.Text = "AUTO"
	elseif state.QueueSize == 0 then
		serveButton.Active = false
		serveButton.AutoButtonColor = false
		serveButton.Text = "NO CUSTOMERS"
	else
		serveButton.Active = true
		serveButton.AutoButtonColor = true
		serveButton.Text = "SERVE"
	end

end

serveButton.MouseButton1Click:Connect(function()
	if serving or not serveButton.Active then return end
	serving = true

	task.delay(0.5, function()
		serving = false
	end)
end)

task.spawn(function()
	while true do
		updateHUD()
		task.wait(1)
	end
end)
]]></ProtectedString>
